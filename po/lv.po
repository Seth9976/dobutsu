#: ../dobutsu.c:718
#, c-format
msgid "Nothing to undo.\n"
msgstr "Nav ko atsaukt.\n"

#: ../dobutsu.c:734
#, c-format
msgid ""
"help        print a list of commands\n"
"hint        print what the engine would play\n"
"exit        leave the program\n"
"version     print program version\n"
"new         start a new game\n"
"undo        undo previous move\n"
"remove      undo last two moves\n"
"setup       setup board with position string\n"
"show board  print the current board\n"
"show setup  print board as a position string\n"
"show moves  print possible moves\n"
"show eval   print position evaluation\n"
"show lines  print possible moves and their evaluations\n"
"strength    show/set engine strength\n"
"both        make engine play both players\n"
"go          make the engine play the colour that is on the move\n"
"force       set the engine to play neither colour\n"
"verbose     print the board after every move\n"
"quiet       do not print the board after every move\n"
msgstr ""
"help        izdod šo pozīciju\n"
"hint        izdod, ko šeit spēlētu dators\n"
"exit        izej no programmas\n"
"version     izdod programmas versiju\n"
"new         iesāc jaunu spēli\n"
"undo        atsauc pēdējo gājienu\n"
"remove      atsauc pēdējos divus gājienus\n"
"setup       ielasi doto īsformāta spēles stāvokli\n"
"show board  izdod spēles laukumu\n"
"show setup  izdod spēles stāvokli īsformātā\n"
"show moves  izdod visus iespējamos gājienus\n"
"show eval   izdod spēles stāvokli un tā novērtējumu\n"
"show lines  izdod iespējamos gājienus un to novērtējumus\n"
"strength    izdod grūtības pakāpi vai izmaini to\n"
"both        atļauj datoram spēlēt abus spēlētājus\n"
"go          atļauj datoram spēlēt spēlētāju, kurš ir klāt\n"
"force       aizliedz datoram spēlēt jebkuru no abiem spēlētājiem\n"
"verbose     izdod spēles laukumu pēc katra gājiena\n"
"quiet       neizdod spēles laukumu pēc katra gājiena\n"


